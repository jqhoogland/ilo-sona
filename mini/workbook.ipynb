   "source": [
    "import yaml\n",
    "\n",
    "def get_vocab(path):\n",
    "    with open(path, 'r') as f: \n",
    "        data = yaml.safe_load(f)\n",
    "    words = [list(entry.keys())[0] for entry in data]\n",
    "    return words\n",
    "\n",
    "# Get words from dictonary file (living in ilosona dir).\n",
    "words = get_vocab(\"../ilosona/dictionary.yaml\")\n"
   ]
  }
